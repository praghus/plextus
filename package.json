{
    "name": "plextus",
    "version": "0.1.1",
    "description": "Pixel art editor designed to make tiled maps and tilesets.",
    "keywords": [
        "react",
        "webpack",
        "typescript",
        "babel",
        "konva",
        "pixel",
        "pixelart",
        "tiled",
        "tmx"
    ],
    "author": "Piotr Praga",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git@github.com:praghus/plextus.git"
    },
    "bugs": {
        "url": "https://github.com/praghus/plextus/issues"
    },
    "homepage": "https://praghus.github.io/plextus",
    "scripts": {
        "build": "yarn run clean-dist && webpack --config=config/webpack/prod.js",
        "clean-dist": "rimraf dist/*",
        "prettier:write": "prettier --write \"{src,config}/**/*.{js,jsx,ts,tsx}\"",
        "prettier:check": "prettier -c \"{src,config}/**/*.{js,jsx,ts,tsx}\"",
        "start": "yarn run start-dev",
        "start-dev": "webpack serve --config=config/webpack/dev.js",
        "start-prod": "yarn run build && node express.js",
        "test": "cross-env NODE_ENV=test jest --coverage",
        "test:clean": "rimraf ./coverage",
        "test:eslint:fix": "npm run test:eslint -- --fix",
        "test:eslint": "eslint --ignore-path .eslintignore --cache --cache-location .eslintcache --ext \".js,.jsx,.ts,.tsx\" src config"
    },
