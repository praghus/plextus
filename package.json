  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "konva",
    "pixel",
    "pixelart",
    "tiled",
    "tmx"
  ],
  "author": "Piotr Praga",
  "license": "MIT",
  "repository": {
